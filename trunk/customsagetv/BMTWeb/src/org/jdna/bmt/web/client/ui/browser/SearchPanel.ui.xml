<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.box {
		padding-bottom: 10px;
	}
	</ui:style>
	<g:VerticalPanel addStyleNames="{style.box}" width="100%" >
		<g:HorizontalPanel addStyleNames="SearchBox" width="100%" ui:field="searchContainer">
			<g:TextBox ui:field="searchBox" width="100%"></g:TextBox>
			<g:Image ui:field="searchButton" url="images/16x16/system-search.png" addStyleNames="clickable"></g:Image>
		</g:HorizontalPanel>
		<g:VerticalPanel ui:field="searchResultsPanel" width="100%">
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 