<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:u="urn:import:org.jdna.bmt.web.client.ui.util">
	<ui:style>
	.image {
		padding: 10 10 10 10;
	}
	</ui:style>
	<g:HTMLPanel>
		<div align="center">
			<u:HorizontalButtonBar ui:field="buttonBar"></u:HorizontalButtonBar>
		</div>
		<div align="center">
			<g:Image ui:field="image" styleName="{style.image}"></g:Image>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 