<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:org.jdna.bmt.web.client.ui.util">
	<ui:style>
		.header {
			font-weight: bold;
			font-size: .75cm;
			padding-top: 5px;
			padding-bottom: 5px;
		}
	</ui:style>
	<g:VerticalPanel width="100%">
		<my:HorizontalButtonBar width="100%">
			<g:Button ui:field="back">Back</g:Button>
			<g:Button ui:field="previous">Previous</g:Button>
			<g:Button ui:field="next">Next</g:Button>
			<g:Button ui:field="save">Save</g:Button>
			<g:Button ui:field="savenext">Save and Next</g:Button>
		</my:HorizontalButtonBar>
		<g:HTMLPanel>
			<g:Label ui:field="title" wordWrap="false" styleName="{style.header}"></g:Label>
		</g:HTMLPanel>
		<g:VerticalPanel ui:field="fieldListPanel"/>
	</g:VerticalPanel>
</ui:UiBinder> 