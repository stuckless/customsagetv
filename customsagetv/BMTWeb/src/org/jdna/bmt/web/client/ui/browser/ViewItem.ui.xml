<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:HorizontalPanel width="100%" spacing="5">
		<g:Label ui:field="label"/>
		<g:cell width="16px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:Image ui:field="createViewMenuAction" url="images/16x16/bookmark-new.png" title="Add View as Menu"/>
		</g:cell>
		<g:cell width="16px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:Image ui:field="hideViewAction" url="images/16x16/list-remove.png" title="Hide View"/>
		</g:cell>
	</g:HorizontalPanel>
</ui:UiBinder> 